.PHONY: all
all: profstats

include ../common.mk

%.o : %.c $(H_FILE_LIST)
	$(CC) $(CFLAGS) $(EXTRA_CFLAGS) $(INCLUDE) -c $< -o $@

profstats: $(OBJS)
	$(CC)  $(LDFLAGS) -o $@ $(OBJS) $(LIBS) $(EXTRA_LDFLAGS)

.PHONY: clean
clean:; rm -fr *.o profstats *.gcov *.gcda *.gcno *.plist

.PHONY: distclean
distclean: clean
	rm -fr Makefile
